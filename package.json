  "scripts": {
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "lint": "eslint config src --ext .js --fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production",
        "API_URL": "http://localhost:8080"
      }
    },
    "build:prod": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production",
        "API_URL": "http://kamilmielnik.com:8080"
      }
    },
    "start": {
      "command": "webpack-wizard start",
      "env": {
        "NODE_ENV": "development",
        "API_URL": "http://localhost:8080"
      }
    },
    "start:prod": {
      "command": "http-server dist",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "better-npm-run": "^0.1.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "http-server": "^0.11.1",
    "react-hot-loader": "^4.0.1",
    "webpack-wizard": "0.1.0-beta.11"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "history": "^2.1.2",
    "js-md5": "0.4.1",
    "moment": "^2.15.0",
    "node-normalize-scss": "^1.3.2",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-datepicker": "0.27.0",
    "react-dom": "^15.3.2",
    "react-icon-base": "2.0.3",
    "react-icons": "^2.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-select": "1.0.0-rc.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "superagent": "^2.2.0",
    "underscore": "^1.8.3"
  }

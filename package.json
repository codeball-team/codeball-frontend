  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/codeball-team/codeball-frontend.git"
  },
  "bugs": "https://github.com/codeball-team/codeball-frontend/issues",
  "scripts": {
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "lint": "eslint config src --ext .js",
    "lint:fix": "eslint config src --ext .js --fix",
    "postinstall": "node scripts/fix-react-datepicker.js",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production",
        "API_URL": "http://localhost:8080"
      }
    },
    "build:prod": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production",
        "API_URL": "http://kamilmielnik.com:8080"
      }
    },
    "start": {
      "command": "webpack-wizard start",
      "env": {
        "NODE_ENV": "development",
        "API_URL": "http://localhost:8080"
      }
    },
    "start:prod": {
      "command": "http-server dist",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "better-npm-run": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "http-server": "^0.11.1",
    "react-hot-loader": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack-wizard": "0.1.0-beta.11"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "history": "^4.7.2",
    "js-md5": "0.7.3",
    "moment": "^2.15.0",
    "node-normalize-scss": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.3.2",
    "react-icon-base": "2.1.2",
    "react-icons": "^2.2.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "1.2.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "superagent": "^3.8.2",
    "underscore": "^1.8.3"
  }
}
